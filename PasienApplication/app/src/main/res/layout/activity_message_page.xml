    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Minta Tolong"
        android:textColor="#1C3B60"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton17"
        app:layout_constraintStart_toEndOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/imageButton17"
        app:layout_constraintVertical_bias="0.441" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Ambilkan Makanan"
        android:textColor="#1C3B60"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton18"
        app:layout_constraintStart_toEndOf="@+id/imageView17"
        app:layout_constraintTop_toTopOf="@+id/imageButton18"
        app:layout_constraintVertical_bias="0.441" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Ambilkan Obat"
        android:textColor="#1C3B60"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton22"
        app:layout_constraintStart_toEndOf="@+id/imageView18"
        app:layout_constraintTop_toTopOf="@+id/imageButton22"
        app:layout_constraintVertical_bias="0.441" />

    <Button
        android:id="@+id/tambahpasien_Button"
        android:layout_width="260dp"
        android:layout_height="60dp"
        android:backgroundTint="#FCCC12"
        android:fontFamily="@font/poppins_bold"
        android:text="+ TAMBAH PESAN"
        android:textColor="#1C3B60"
        android:textSize="20sp"
        app:cornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton22" />


</androidx.constraintlayout.widget.ConstraintLayout>